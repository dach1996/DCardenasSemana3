<?xml version="1.0" encoding="utf-8" ?>
<ContentPage xmlns="http://xamarin.com/schemas/2014/forms"
             xmlns:x="http://schemas.microsoft.com/winfx/2009/xaml"
             x:Class="DCardenasSemana3.MainPage">

    <StackLayout>
        <Label Text="Esta es la ventana 1"></Label>
        <Entry x:Name="txtUser" Placeholder="Ingrese El Usuario"></Entry>
        <Entry x:Name="textPass" Placeholder="Ingrese Password" IsPassword="True"></Entry>
        <Button x:Name="btnAbrirVentanaDos" Text="Abrir Ventana 2" Clicked="btnAbrirVentanaDos_Clicked"></Button>
        
    </StackLayout>

</ContentPage>
